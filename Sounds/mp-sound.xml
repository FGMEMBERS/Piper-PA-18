<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <fx>
    
    <crank>
      <name>engstart</name>
      <path>Aircraft/Piper-PA-18/Sounds/fk9_eng_start_ext1_part1.wav</path>
      <condition>
        <and>
          <property>/engines/engine/cranking</property>
          <property>/sim/current-view/internal</property>
        </and>
      </condition>
      <volume>
        <offset>0.4</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>
    
    <crank>
      <name>engstartext</name>
      <path>Aircraft/Piper-PA-18/Sounds/fk9_eng_start_int1_part1.wav</path>
      <condition>
        <and>
          <property>/engines/engine/cranking</property>
          <not>
            <property>/sim/current-view/internal</property>
          </not>
        </and>
      </condition>
      <volume>
        <offset>0.4</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>
    
    <crank>
      <name>crank</name>
      <mode>looped</mode>
      <path>Aircraft/Piper-PA-18/Sounds/fk9_eng_start_int1_part2.wav</path>
      <condition>
        <and>
          <property>/engines/engine/cranking</property>
          <property>/sim/current-view/internal</property>
          <not>
            <property>/engines/engine/running</property>
          </not>
        </and>
      </condition>
      <volume>
        <offset>0.5</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>
    
    <crank>
      <name>crankext</name>
      <mode>looped</mode>
      <path>Aircraft/Piper-PA-18/Sounds/fk9_eng_start_ext1_part2.wav</path>
      <condition>
        <and>
          <property>/engines/engine/cranking</property>
          <not>
            <property>/sim/current-view/internal</property>
          </not>
          <not>
            <property>/engines/engine/running</property>
          </not>
        </and>
      </condition>
      <volume>
        <offset>0.5</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>
    
    <crank>
      <name>cough</name>
      <path>Sounds/coughing.wav</path>
      <property>/engines/engine/running</property>
      <volume>
        <offset>0.3</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>
    
    <stopext>
      <name>stopext</name>
      <path>Aircraft/Piper-PA-18/Sounds/fk9_eng_stop_ext1.wav</path>
      <condition>
        <not>
          <property>/engines/engine/running</property>
        </not>
      </condition>
      <volume>
        <property>/sim/current-view/internal</property>
        <factor>-1.0</factor>
        <offset>1.0</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </stopext>
    
    <stop>
      <name>stop</name>
      <path>Aircraft/Piper-PA-18/Sounds/fk9_eng_stop_int1.wav</path>
      <condition>
        <not>
          <property>/engines/engine/running</property>
        </not>
      </condition>
      <volume>
        <property>/sim/current-view/internal</property>
        <factor>1.0</factor>
        <offset>0.0</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </stop>
    
    <engine>
      <name>engine</name>
      <mode>looped</mode>
      <path>Aircraft/Piper-PA-18/Sounds/fk9_eng_run_ext1_loop.wav</path>
      <condition>
        <and>
          <property>/engines/engine/running</property>
          <not>
            <property>/sim/current-view/internal</property>
          </not>
        </and>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine/rpm</property>
        <factor>0.0009496</factor>
        <min>0.6</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>
    
    <engine>
      <name>engineextslow</name>
      <mode>looped</mode>
      <path>Aircraft/Piper-PA-18/Sounds/fk9_eng_run_ext3_slow.wav</path>
      <condition>
        <and>
          <property>/engines/engine/running</property>
          <not>
            <property>/sim/current-view/internal</property>
          </not>
        </and>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine/rpm</property>
        <factor>0.0009496</factor>
        <min>0.6</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>
    
    <propellerhigh>
      <name>propellerhigh</name>
      <mode>looped</mode>
      <path>Aircraft/Piper-PA-18/Sounds/fk9_eng_run_int_4430.wav</path>
      <condition>
        <and>
          <property>/engines/engine/running</property>
          <property>/sim/current-view/internal</property>
        </and>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine/rpm</property>
        <factor>0.0009496</factor>
        <min>0.6</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </propellerhigh>
    
    <propellerlow>
      <name>propellerlow</name>
      <mode>looped</mode>
      <path>Aircraft/Piper-PA-18/Sounds/fk9_eng_run_int_1340.wav</path>
      <condition>
        <and>
          <property>/engines/engine/running</property>
          <property>/sim/current-view/internal</property>
        </and>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine/rpm</property>
        <factor>0.0009496</factor>
        <min>0.6</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </propellerlow>
    
    <rumble>
      <name>rumble</name>
      <mode>looped</mode>
      <path>Aircraft/Piper-PA-18/Sounds/fk9_rumble.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
        <!-- <property>/sim/current-view/internal</property> -->
      </condition>
      <volume>
        <property>/velocities/groundspeed-kt</property>
        <!--type>log</type-->
        <factor>0.025</factor>
        <offset> -0.01</offset>
        <max>1.01</max>
        <min>0.01</min>
      </volume>
      <pitch>
        <property>/velocities/groundspeed-kt</property>
        <factor>0.01</factor>
        <offset>0.85</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </rumble>
    
    <squeal>
      <name>squeal</name>
      <path>Aircraft/Piper-PA-18/Sounds/fk9_wow.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <property>/velocities/speed-down-fps</property>
        <factor>0.5</factor>
        <max>1.0</max>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.1</factor>
        <max>1.0</max>
      </volume>
      <volume>
        <internal>dt_stop</internal>
        <factor>0.05</factor>
        <max>1.0</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0025</factor>
        <offset>1.0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </squeal>
    
    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/flap-pos-norm</property>
      <volume>
        <factor>0.8</factor>
      </volume>
      <reference-dist>10.0</reference-dist>
      <max-dist>20.0</max-dist>
    </flap>
    
    <wind>
      <name>wind</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <property>/velocities/airspeed-kt</property>
      <volume>
        <property>/position/altitude-ft</property>
        <factor>-0.000015</factor>
        <offset>1.0</offset>
        <min>0.0</min>
        <max>1.0</max>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0015</factor>
        <min>0.0</min>
        <max>0.25</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0035</factor>
        <offset>1.25</offset>
      </pitch>
    </wind>
    
  </fx>
  
</PropertyList>
